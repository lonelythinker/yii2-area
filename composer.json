{
    "name": "lonelythinker/yii2-area",
    "description": "Yii2 area extension",
    "keywords": ["lonelythinker", "yii2", "extension", "area", "region", "province", "city", "county", "plugin"],
    "homepage": "https://github.com/lonelythinker/yii2-area",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "version": "0.0.1",
    "authors": [
        {
            "name": "lonelythinker",
            "email": "710366112@qq.com",
            "homepage": "http://www.lonelythinker.cn/",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-imagine": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "autoload": {
        "psr-4": {
            "lonelythinker\\yii2\\area\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
